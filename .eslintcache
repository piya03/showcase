e, useEffect } from \"react\";\nimport ReactDOM from \"react-dom\";\nimport {\n  BrowserRouter as Router,\n  Switch,\n  Route,\n  useParams,\n} from \"react-router-dom\";\nimport \"./App.css\";\nimport Paper from \"./TestPaper/Paper\";\nimport UserForm from \"./UserForm/UserForm\";\nimport ComponentA from \"./Context/ComponentA\";\nimport StopTimer from \"./StopTimer/StopTimer\";\nimport StudentDashboard from \"./StudentDashboard/StudentDashboard\";\nimport Links from \"./Link/Links\";\nfunction App() {\n  const [col, setCol] = useState(0);\n  const [row, setRow] = useState(0);\n  const [tabledata, setTableData] = useState([]);\n\n  const [showNo, setShowNo] = useState(\"\");\n\n  const getUniqueKeyFromArrayIndex = (rowNum, columnNum) => {\n    return `${rowNum}-${columnNum}`;\n  };\n\n  function isFloat(n) {\n    return Number(n) === n && n % 1 !== 0;\n  }\n\n  const generate = (row, col) => {\n    if (row < 1 || col < 1) {\n      alert(\"Row and Col cannot be zero\");\n\n      return;\n    }\n    if (isFloat(row) || isFloat(col)) {\n      alert(\"Float Number are not allowed\");\n      return;\n    }\n    let table = [];\n    // Outer loop to create parent\n\n    let count = 1;\n    for (let i = 0; i < row; i++) {\n      let children = [];\n      //Inner loop to create children\n      for (let j = 0; j < col; j++) {\n        let val = count;\n        children.push(\n          <td name={getUniqueKeyFromArrayIndex(i, j)} defaultValue=\"TextInput\">\n            <button\n              onClick={() => {\n                setShowNo(val);\n              }}\n            >\n              {count++}\n            </button>\n          </td>\n        );\n      }\n      table.push(<tr key={i}>{children}</tr>);\n    }\n    setTableData(table);\n    return table;\n  };\n\n  return (\n    <div>\n      <div className=\"container\">\n        <div className=\"left_box\">\n          <h2>Block Display</h2>\n          <table>\n            <tbody>{tabledata}</tbody>\n          </table>\n          <div>\n            <h2>Total Blocks : {row * col}</h2>\n          </div>\n        </div>\n        <div>\n          <h2>Block Config</h2>\n          <div className=\"row_col\">\n            <div className=\"row\">\n              <div> Row :</div>\n              <input\n                type=\"number\"\n                value={row}\n                onChange={(e) => setRow(e.target.value)}\n              />\n            </div>\n\n            <div className=\"col\">\n              <div> Column : </div>\n              <input\n                type=\"number\"\n                value={col}\n                onChange={(e) => setCol(e.target.value)}\n              />\n            </div>\n          </div>\n          <div>\n            <button onClick={() => generate(row, col)}>Generate</button>\n            <button\n              onClick={() => {\n                setRow(0);\n                setCol(0);\n                setTableData([]);\n                setShowNo(\"\");\n              }}\n            >\n              Clear\n            </button>\n          </div>\n          <div></div>\n          <div>\n            <h1>Selected Block</h1>\n            <div>{showNo}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/cepl/practice/showcase/src/TestPaper/Paper.js",[],["82","83"],"/Users/cepl/practice/showcase/src/CommonComponent/CommonCheckBox.js",[],"/Users/cepl/practice/showcase/src/UserForm/UserForm.js",["84","85","86","87","88","89","90"],"import React, { useState } from \"react\";\nimport useInput from \"../Hooks/useInput\";\nconst UserForm = () => {\n  const [FirstName, bindFirstName, resetFirstName] = useInput(\"\");\n  const [lastName, bindLastName, resetLastName] = useInput(\"\");\n  const [address, bindAddress, resetAddress] = useInput(\"\");\n  const [pincode, bindPincode, resetPincode] = useInput(\"\");\n\n  let as_variable_markUp = {\n    __html: \"setting this dangerouslyInnerHTML as variable\",\n  };\n  f